{
    "format_version": "1.12.0",
    "minecraft:client_entity": {
        "description": {
            "identifier": "nv_ft:villager_common",
            "materials": {
                "default": "player_animated"
            },
            "textures": {
                "variant_1": "textures/nv/ft/entity/villager/villager1",
                "variant_2": "textures/nv/ft/entity/villager/villager2",
                "variant_3": "textures/nv/ft/entity/villager/villager3",
                "variant_4": "textures/nv/ft/entity/villager/villager4",
                "variant_5": "textures/nv/ft/entity/villager/villager5"
            },
            "geometry": {
                "variant_1": "geometry.nv_ft.villager_1",
                "variant_2": "geometry.nv_ft.villager_2",
                "variant_3": "geometry.nv_ft.villager_3",
                "variant_4": "geometry.nv_ft.villager_4",
                "variant_5": "geometry.nv_ft.villager_5"
            },
            "animations": {
                "blink_controller": "controller.animation.nv_ft.villager_blink",
                "controller": "controller.animation.nv_ft.villager",
                "look_at_target": "animation.common.look_at_target",
                "walk": "animation.nv_ft.villager.walk",
                "idle": "animation.nv_ft.villager.idle",
                "death": "animation.nv_ft.villager.death"
            },
            "scripts": {
                "initialize": [
                    "v.frame_count = q.variant == 1 ? 16 : 12;",
                    "v.blink_interval = math.random(1.5, 5);",
                    "v.blink = false;",
                    "v.uv_frame = 0;"
                ],
                "pre_animation": [
                    "v.uv_frame = v.blink ? math.floor(q.life_time * 12) : 0;"
                ],
                "animate": [
                    { "blink_controller": "!q.property('nv_ft:dead')" },
                    { "controller": "!q.property('nv_ft:dead')" },
                    { "death": "q.property('nv_ft:dead')" }
                ]
            },
            "spawn_egg": {
                "base_color": "#e2b497",
                "overlay_color": "#361d0c"
            },
            "render_controllers": [ "controller.render.nv_ft.villager" ]
        }
    }
}